{
    "version": "0.2.0",
    "configurations": [
        {
            "name": ".NET Core Launch (console)",
            "type": "coreclr",
            "request": "launch",
            "preLaunchTask": "build",
            "program": "${workspaceFolder}/HangMan-Prj/bin/Debug/net7.0/HangMan-Prj.dll",
            "args": [],
            "cwd": "${workspaceFolder}/HangMan-Prj",
            "console": "internalConsole",
            "stopAtEntry": false
        },
        {
            "name": ".NET Core Attach",
            "type": "coreclr",
            "request": "attach"
        },
        {
            "name": "Docker .NET Launch",
            "type": "docker",
            "request": "launch",
            "preLaunchTask": "docker-run: debug",
            "netCore": {
                "appProject": "${workspaceFolder}/Prj1_App_Folder/Prj1Api/Prj1_Api/Prj1_Api.csproj"
            }
        }
    ]
}